[window]
width = 580
height = 520
anchor = center
margin_top = 0
margin_bottom = 0
margin_left = 0
margin_right = 0

[style]
theme = default

[behavior]
max_items = 0
close_on_select = true
notify_on_copy = false
vim_mode = false

[keybinds]
select = Return Enter
delete = Delete
clear_search = Ctrl+u
close = Escape
next = Down Tab
prev = Up Shift+Tab
page_down = Page_Down
page_up = Page_Up
first = Home
last = End

